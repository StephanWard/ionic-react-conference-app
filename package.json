{
  "name": "ionic-react-conference-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "npm-run-all -p tsc 'vite build'",
    "preview": "vite preview",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,md}'",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "precommit": "npm run lint && lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@capacitor/core": "^2.4.6",
    "@capacitor/preferences": "^2.4.6",
    "@ionic/react": "^6.0.0",
    "@ionic/react-router": "^6.0.0",
    "date-fns": "^2.25.0",
    "ionicons": "^7.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "reselect": "^4.0.0",
    "sass": "^1.59.3",
    "swiper": "^9.1.1"
  },
  "devDependencies": {
    "@capacitor/cli": "^2.4.6",
    "@testing-library/react": "^11.2.0",
    "@types/googlemaps": "^3.38.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^7.32.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.6",
    "typescript": "^4.4.4",
    "vite": "^4.3.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": ["react-app", "prettier"]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC"
}